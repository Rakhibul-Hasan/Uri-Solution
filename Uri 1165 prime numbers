#include<stdio.h>
int main()
{
	int n,i,a,c,j;
	scanf("%d",&n);
	for(i=0;i<n;i++){
		scanf("%d",&a);
		if(a==1){ printf("%d nao eh primo\n",a);}
		else if(a==2){ printf("%d eh primo\n",a);}
		else if(a>2){ for(j=2;j<a;j++){ if(a%j==0){
			c=1;
			break ;
		}
		else{c=2;}
		}
		
		if(c==1){ printf("%d nao eh primo\n",a);}
		else if(c==2){printf("%d eh primo\n",a);}
		}}
	
}
